<%= Invoice.model_name.human %>, Client, Grand Total, <%= current_account.tax1_label.present? ? current_account.tax1_label : 'Tax1' %>, <%= current_account.tax2_label.present? ? current_account.tax2_label : 'Tax2' %>, Total
<% @invoices.order('created_at DESC').each do |invoice| %>
<%= invoice.code %>, <%= invoice.quote.client.name %>, <%= invoice.grand_total %>, <%= invoice.tax1_amount %>, <%= invoice.tax2_amount %>, <%= invoice.total %>
<% end %>