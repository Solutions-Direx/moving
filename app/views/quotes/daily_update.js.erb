<% if @quote.errors.blank? %>
var html = "<p class='alert alert-success'><%= ("#{t 'quotes.daily.success_update', default: 'Successfully saved.'}") %></p>";
<% else %>
var html = "<p class='alert alert-error'><%= ("#{t 'quotes.daily.error_update', default: 'An error occured. Please try again and contact system admininistrator.'}") %></p>";
<% end %>  
$(html).dialog2({ 
    id: "alert2",
    autoOpen: true,
    buttons: {
        Close: { 
            primary: true, 
            click: function() {
                $(this).dialog2("close");
            }
        }
    }, 
    removeOnClose: true
});