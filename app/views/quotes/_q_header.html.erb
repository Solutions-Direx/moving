<p>
  <b><%= t 'removal_at' %>:</b>
  <%= l quote.removal_at, :format => :long %> || <%= quote.is_house? ? "#{t 'house'}" : "#{t 'apartment'}" %>
  <span class='submenu'><b><%= t 'created_by', default: 'Created by' %>: </b><%= quote.creator.full_name %></span>
</p>

<% if quote.pm? %>
  <p>
    <span class='label label-warning label_override'><%= t 'removal_time_confirmation' %></span>
    <% if quote.removal_at_comment.present? %><%= quote.removal_at_comment %><% end %>
  </p>
<% end %>